:javascript
  $(document).ready(function(){
    $('.about-box-panel').hide();

    $('.about-list-circles a').bind('click', function(e){
      $('.about-list-circles a.current').removeClass('current');
      $('.about-box-panel:visible').hide();
      $(this.hash).fadeIn('2000');
      $(this).addClass('current');
      e.preventDefault();
    }).filter(':first').click();

  });
